}

% #1 is option list
% #2 is player name
% (#3,#4) direction of branch
% #5 is action label
\def\@ctmb#1#2(#3,#4)#5{%
	\@egctmsethv{#3}{#4}
	\@ifnextchar[{\@ctmbot{#1}{#2}{t}{#5}}{\@ctmbotn{#1}{#2}{t}{#5}}%
}

